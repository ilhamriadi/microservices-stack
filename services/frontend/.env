# Build Environment
NODE_ENV=production

# API Endpoints
VITE_API_BASE_URL=http://156.67.24.197/api
VITE_AUTH_BASE_URL=http://156.67.24.197/auth
VITE_CATALOG_BASE_URL=http://156.67.24.197/catalog

# Application Configuration
VITE_APP_NAME="Microservices App"
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Modern Microservices Application"

# Authentication
VITE_JWT_TOKEN_KEY=authToken
VITE_REFRESH_TOKEN_KEY=refreshToken
VITE_TOKEN_EXPIRY_KEY=tokenExpiry

# Features
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_HOT_RELOAD=false

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_CACHE_STRATEGY=networkFirst

# Theme
VITE_DEFAULT_THEME=light
VITE_THEME_COLOR=#1976d2

# Development
VITE_DEV_SERVER_HOST=0.0.0.0
VITE_DEV_SERVER_PORT=3000
